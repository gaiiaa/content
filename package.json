{
  "name": "@gaiiaa/content",
  "version": "0.1.2",
  "description": "Content pipeline for Vite",
  "repository": "@gaiiaa/content",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx --watch src",
    "build": "unbuild",
    "lint": "oxlint && prettier -w src test playground",
    "release": "changelogen --release && npm publish && git push --follow-tags"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.6",
    "changelogen": "^0.5.5",
    "oxlint": "^0.1.2",
    "prettier": "^3.0.3",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "packageManager": "pnpm@8.10.0",
  "dependencies": {
    "@types/unist": "^3.0.2",
    "hast-util-to-jsx-runtime": "^2.3.0",
    "js-yaml": "^4.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-directive": "^3.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0"
  },
  "peerDependencies": {
    "vite": "^5.0.11"
  },
  "unbuild": {
    "declaration": true,
    "entries": [
      {
        "builder": "mkdist",
        "format": "esm",
        "input": "./src",
        "ext": ".js",
        "outDir": "./dist"
      }
    ]
  }
}